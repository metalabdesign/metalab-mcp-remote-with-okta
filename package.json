{
  "name": "@metalabdesign/metalab-mcp-remote-with-okta",
  "version": "1.3.0",
  "description": "MCP remote server with Okta authentication wrapper",
  "main": "src/index.js",
  "bin": {
    "mcp-remote-with-okta": "dist/index.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/index.js",
    "lint:fix": "eslint src/index.js --fix",
    "build:local-app": "node build.js",
    "build:install": "esbuild install.js --bundle --platform=node --target=node22 --outfile=dist/install.js --banner:js='#!/usr/bin/env node' && chmod +x dist/update-mcp.js",
    "start": "node build.js && node dist/index.js"
  },
  "keywords": [
    "mcp",
    "adobe",
    "okta",
    "ims",
    "authentication",
    "oauth",
    "remote",
    "model-context-protocol",
    "claude",
    "cursor"
  ],
  "author": "Metalab Design",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metalabdesign/metalab-mcp-remote-with-okta.git"
  },
  "bugs": {
    "url": "https://github.com/metalabdesign/metalab-mcp-remote-with-okta/issues"
  },
  "homepage": "https://github.com/metalabdesign/metalab-mcp-remote-with-okta#readme",
  "dependencies": {
    "jsonc-parser": "^3.3.1",
    "mcp-remote": "^0.1.12"
  },
  "devDependencies": {
    "dotenv": "^17.2.1",
    "esbuild": "^0.25.9",
    "eslint": "^8.57.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "index.js",
      "auth-strategy.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
