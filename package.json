{
  "name": "@metalabdesign/metalab-mcp-remote-with-okta",
  "version": "1.3.0",
  "description": "MCP remote server with Okta authentication wrapper",
  "main": "src/index.js",
  "bin": {
    "mcp-remote-with-okta": "dist/index.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/index.js",
    "lint:fix": "eslint src/index.js --fix",
    "build:local-app": "node build.js",
    "build:local-app:watch": "node --watch-path=src build.js",
    "build:install": "webpack --config webpack.config.js && chmod +x dist/install.js",
    "build": "pnpm build:local-app && pnpm build:install",
    "start": "pnpm build:local-app && node dist/index.js",
    "debug": "npx -y @modelcontextprotocol/inspector"
  },
  "keywords": [
    "mcp",
    "adobe",
    "okta",
    "ims",
    "authentication",
    "oauth",
    "remote",
    "model-context-protocol",
    "claude",
    "cursor"
  ],
  "author": "Metalab Design",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metalabdesign/metalab-mcp-remote-with-okta.git"
  },
  "bugs": {
    "url": "https://github.com/metalabdesign/metalab-mcp-remote-with-okta/issues"
  },
  "homepage": "https://github.com/metalabdesign/metalab-mcp-remote-with-okta#readme",
  "dependencies": {
    "jsonc-parser": "^3.3.1",
    "mcp-remote": "^0.1.12"
  },
  "devDependencies": {
    "dotenv": "^17.2.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "index.js",
      "auth-strategy.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
